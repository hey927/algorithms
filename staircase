#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    int n;
    cin >> n;

    
    int j=1;
    for(int i=n-1; i>=0; i--){
        std::cout << std::string(i,' ') + std::string(j, '#') << std::endl;
        j++;
    }
    
    return 0;
}
